<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.web.mapper.UsertokenMapper">

    <select id="selectByUserid"   resultType="com.web.entity.Usertoken"   >

        select * from t_usertoken where userid=#{userid}

    </select>

    <select id="selectByToken"   resultType="com.web.entity.Usertoken"   >

        select * from t_usertoken where token=#{token}

    </select>


</mapper>







